{
    "name": "oneofftech/laravel-language-recognizer",
    "description": "Recognize language in text",
    "keywords": [
        "oneofftech",
        "laravel",
        "language detection",
        "language"
    ],
    "homepage": "https://github.com/oneofftech/laravel-language-recognizer",
    "license": "MIT",
    "authors": [
        {
            "name": "Alessio",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^8.37|^9.0",
        "illuminate/support": "^8.37|^9.0",
        "illuminate/http": "^8.37|^9.0",
        "symfony/process": "^5.1.4|^6.0"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^4.8",
        "guzzlehttp/guzzle": "^7.0.1"
    },
    "autoload": {
        "psr-4": {
            "Oneofftech\\LaravelLanguageRecognizer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Oneofftech\\LaravelLanguageRecognizer\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Oneofftech\\LaravelLanguageRecognizer\\LaravelLanguageRecognizerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
